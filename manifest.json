{
  "name": "Поиск по закладкам",
  "version": "1.1",
  "description": "Поиск по всем закладкам браузера",
  "permissions": [
    "bookmarks"
  ],
  "browser_action": {
      "default_title": "Bookmarks",
      "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}